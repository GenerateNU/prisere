{
  "name": "backend",
  "scripts": {
    "migration:gen": "NODE_ENV=development bunx --bun typeorm-ts-node-esm migration:generate",
    "migration:dev": "NODE_ENV=development bunx --bun typeorm-ts-node-esm migration:run -d ./src/typeorm-config.ts",
    "migration:dev:revert": "NODE_ENV=development bunx --bun typeorm-ts-node-esm migration:revert -d ./src/typeorm-config.ts",
    "migration:prod": "NODE_ENV=production bunx --bun typeorm-ts-node-esm migration:run -d ./src/typeorm-config.ts",
    "migration:prod:revert": "NODE_ENV=production bunx --bun typeorm-ts-node-esm migration:revert -d ./src/typeorm-config.ts",
    "dev": "NODE_ENV=development bun --hot --no-clear-screen run src/server.ts",
    "start": "NODE_ENV=production bun src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target bun",
    "start:built": "NODE_ENV=production bun dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --config src/eslint.config.ts && prettier . --check",
    "lint:fix": "eslint . --config src/eslint.config.ts --fix && prettier . --write"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@aws-sdk/client-ses": "^3.914.0",
    "@aws-sdk/s3-request-presigner": "^3.917.0",
    "@hapi/boom": "^10.0.1",
    "@hono/node-server": "^1.12.2",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@react-email/components": "^1.0.1",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "@tanstack/react-table": "^8.21.3",
    "adm-zip": "^0.5.16",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "client-sqs": "aws-sdk/client-sqs",
    "cron": "^4.3.3",
    "csrf": "^3.1.0",
    "dayjs": "^1.11.18",
    "hono": "^4.5.10",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.4.5",
    "pg": "^8.16.3",
    "pg-mem": "^3.0.5",
    "puppeteer": "^24.29.1",
    "react-pdf": "^10.2.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.4",
    "supabase": "^2.39.2",
    "terraform": "^1.23.0",
    "typeorm": "^0.3.26",
    "typeorm-extension": "^3.7.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.1.0",
    "@hono/vite-dev-server": "^0.17.0",
    "@types/adm-zip": "^0.5.7",
    "@types/aws-lambda": "^8.10.156",
    "@types/bun": "^1.2.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.12",
    "@types/papaparse": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "vite": "^5.4.2"
  }
}
